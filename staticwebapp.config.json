{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    },
    {
      "navigationFallback": {
        "route": "/*",
        "rewrite": "/index.html",
        "exclude": [
          "/assets/*",
          "/images/*.{png,jpg,gif}",
          "/css/*"
        ]
      }
    }
  ]
}