{
  "routes": [
    {
      "route": "/assets/*",
      "methods": [
        "get"
      ]
    },
    {
      "route": "/config.js",
      "methods": [
        "get"
      ]
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    },
    {
      "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
          "/assets/*",
          "/images/*.{png,jpg,gif}",
          "/css/*"
        ]
      }
    }
  ]
}