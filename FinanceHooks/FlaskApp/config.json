{
    "image" : [
        {
            "name": "gcash_express",
            "app":"gcash",
            "conditions": [
                {
                    "hasLine": "Express Send"
                }
            ],
            "properties": [
                {
                    "property": "recipientName",
                    "lookFor": "Express Send",
                    "getValueAfter" : 1
                },
                {
                    "property": "recipientAcct",
                    "lookForRegex": "\\+63( [0-9·]+){3,4}",
                    "getValueAfter" : 0,
                    "replaceRegex":[{"f":"·", "t":"*"}, {"f":" ", "t":""}]
                },
                {
                    "property": "reference",
                    "lookForRegex":"Ref No\\. ([0-9 ]+)",
                    "getValueAfter" :0,
                    "extractRegex":"Ref No\\. ([0-9 ]+)",
                    "getMatch":1,
                    "removeRegex":[" "]
                },
                {
                    "property":"amount",
                    "lookFor": "Amount",
                    "getValueAfter":1,
                    "removeRegex": [","]
                }
            ]
        },
